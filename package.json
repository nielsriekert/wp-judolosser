{
  "name": "judolosser",
  "version": "1.6.1",
  "description": "WordPress theme for Judo Losser",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run docker && npm run dev",
    "dev": "webpack --progress --config webpack.development.js --watch",
    "production": "webpack --config webpack.production.js",
    "translation": "webpack --mode production",
    "docker": "docker-compose up -d",
    "docker-remove": "docker stop $(docker ps -a -q) && docker rm $(docker ps -a -q)",
    "server-start": "docker-compose start",
    "server-stop": "docker-compose stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nielsriekert/wp-judolosser.git"
  },
  "author": "Niels Riekert",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@sanity/client": "^0.140.36",
    "@webcomponents/template": "^1.4.0",
    "flickity": "^2.2.1",
    "lit-html": "^1.1.0",
    "normalize.css": "^8.0.1",
    "vanillelightbox": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "img-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-merge": "^4.2.1"
  }
}
