{
  "name": "judolosser",
  "version": "1.1.3",
  "description": "WordPress theme for Judo Losser",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "translation": "webpack --mode production",
    "production": "NODE_ENV=production webpack",
    "docker": "docker-compose up -d",
    "docker-remove": "docker stop $(docker ps -a -q) && docker rm $(docker ps -a -q)",
    "server-start": "docker-compose start",
    "server-stop": "docker-compose stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nielsriekert/wp-judolosser.git"
  },
  "author": "Niels Riekert",
  "license": "ISC",
  "dependencies": {
    "normalize.css": "^7.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "img-loader": "^2.0.1",
    "node-sass": "^4.8.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 3 versions"
            ]
          }
        }
      ]
    ]
  }
}
